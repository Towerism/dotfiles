[
  {
    "clean": ["~"]
  },
  {
    "shell":
    [
      ["git pull", "Pulling latest commits"],
      ["cd emacs.d && cask install", "Installing emacs lisp packages"]
    ]
  },
  {
    "link":
    {
      "~/.emacs.d": "emacs.d",
      "~/.zshrc": "zshrc",
      "~/.Xresources": "Xresources",
      "~/.xinitrc": "xinitrc",
      "~/.config/i3": "i3",
      "~/.config/i3status": "i3status"
    }
  }
]
